{
  "plugins": ["@salesforce/eslint-plugin-aura"],
  "extends": ["plugin:@salesforce/eslint-plugin-aura/recommended", "prettier"],
  "rules": {
    "func-names": "off",
    "vars-on-top": "off",
    "no-unused-expressions": "off",
    "newline-before-return": "off",
    "padding-line-between-statements": "off",
    "no-console": "off"
  }
}
